---
import "../css/main.css";
import MainPage from "../components/MainPage.astro";
import SimpleGallery from "../components/SimpleGallery.astro";
---

<MainPage>
  <section id="inicio" class="relative">
    <img
      src="toyota-landing-page.jpg"
      class="object-cover object-[100%,100%] -z-10 hidden sm:block"
    />
    <img
      src="toyota-landing-page-phone.jpg"
      class="object-cover object-[100%,100%] -z-10 sm:hidden"
    />
  </section>
  <section class="">
    <img src="/agya-logo-black.png" class="w-1/3 m-auto pb-8 pt-20" />
    <SimpleGallery />
  </section>
</MainPage>
